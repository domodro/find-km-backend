{
  "app": {
    "port": 8080
  },
  "osm": {
    "host": "z.overpass-api.de",
    "path": "/api/interpreter",
    "query": "[out:json];rel(49715);map_to_area;node(area)[highway=milestone];out;",
    "separator": ";"
  },
  "roads": [
    {
      "regex": "^[Aa][0-9]{1,2}[a-z]?$",
      "roadClass": "HIGHWAY"
    },
    {
      "regex": "^[Ss][0-9]{1,2}[a-z]?$",
      "roadClass": "EXPRESSWAY"
    },
    {
      "regex": "^[0-9]{1,2}[a-z]?$",
      "roadClass": "NATIONAL"
    },
    {
      "regex": "^[0-9]{3}[a-z]?$",
      "roadClass": "REGIONAL"
    },
    {
      "regex": "^[0-9]{4}[A-Z]$",
      "roadClass": "DISTRICT"
    }
  ]
}
